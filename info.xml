<?xml version="1.0"?>
<extension key="uk.org.bec.custom" type="module">
  <file>custombec</file>
  <name>BEC Customisations</name>
  <description>Implements customisations for BEC such as tokens</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Various</author>
    <email>mjw@mjwconsult.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Support">https://mjw.pt/support/beccustom</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-10-07</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.65</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Bec</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
